{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri",
  "version": "0.1.0",
  "identifier": "swingAnt",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "center": false,
        "width": 800,
        "height": 100,
      "decorations": false,
        "minimizable": true,
        "x": 0,
        "y":0,
        "shadow": true,
        "title": "Code AI",
        "transparent": true,
        "windowEffects": {
          "effects": [],
          "radius": 60
        }

      }
    ],
    "security": {
      "csp": null
    }

  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
